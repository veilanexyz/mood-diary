{
    "name": "FastAPI Mood Diary",
    "image": "mcr.microsoft.com/vscode/devcontainers/python:3.9",
    "settings": {
      "terminal.integrated.shell.linux": "/bin/bash"
    },
    "extensions": [
      "ms-python.python",
      "ms-azuretools.vscode-docker"
    ],
    "postCreateCommand": "pip install -r requirements.txt",
    "forwardPorts": [8000],
    "remoteUser": "vscode"
  }
  